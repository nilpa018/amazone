{% extends 'base.html.twig' %}

{% block title %}Visitor{% endblock %}

{% block body %}
    <h1>Visitor</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ visitor.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ visitor.name }}</td>
            </tr>
            <tr>
                <th>Adress</th>
                <td>{{ visitor.adress }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ visitor.phone }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_visitor_index') }}">back to list</a>

    <a href="{{ path('app_visitor_edit', {'id': visitor.id}) }}">edit</a>

    {{ include('visitor/_delete_form.html.twig') }}
{% endblock %}
